package ssen.framework.widgets.btns {import ssen.framework.service.SimpleLauncher;import ssen.framework.widgets.core.IWidget;public class BtnExample extends SimpleLauncher {	override protected function getApplication() : IWidget {		return new App;	}}}import ssen.core.DisplayX;import ssen.framework.widgets.btns.Btn;import ssen.framework.widgets.btns.ToggleBtnInteraction;import ssen.framework.widgets.core.SpriteWidget;import ssen.framework.widgets.core.WidgetState;class App extends SpriteWidget {	private var btn1 : Btn;
	private var btn2 : Btn;
	override protected function wregister() : void {		btn1 = new Btn;		btn1.construct();		btn1.setting_Btn(sprite, -1, WidgetState.RUN, 100, 30);		btn1.register();				btn2 = new Btn;		btn2.construct();		btn2.setting_Btn(sprite, -1, WidgetState.RUN, 100, 30, null, new ToggleBtnInteraction);		btn2.register();				DisplayX.align([btn1, btn2], 10, 10);				addToService();	}}