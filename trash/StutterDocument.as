package ssen.stutter {
import ssen.framework.widgets.core.Widget;

/**
 * @author ssen (i@ssen.name)
 */
public class StutterDocument extends Widget {
	private var _style : StutterStyle;

	public function get height() : int {
		return 0;
	}

	public function get width() : int {
		return 800;
	}

	public function get headerMargin() : int {
		return 0;
	}

	public function get style() : StutterStyle {
		if (!_style) _style = new StutterStyle();
		return _style;
	}
}
}
